{
  "name": "@bryce-loskie/scrollbar",
  "version": "0.0.1-beta.3",
  "description": "Opinionated scrollbar style for the web",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "dev": "pnpm build -- --watch",
    "build": "tsup",
    "start": "esno src/index.ts"
  },
  "keywords": [
    "scrollbar"
  ],
  "author": "guygubaby@gmail.com",
  "license": "MIC",
  "devDependencies": {
    "@bryce-loskie/utils": "^0.1.28"
  }
}
